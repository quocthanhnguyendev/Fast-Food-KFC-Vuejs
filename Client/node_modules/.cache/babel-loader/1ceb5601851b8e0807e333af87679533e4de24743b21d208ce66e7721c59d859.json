{"ast":null,"code":"import { GetCategories } from \"../API/CategoriesRequest\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"ShopPage\",\n  data() {\n    return {\n      isLayout: true,\n      Products: [],\n      Categories: [],\n      categoryActive: 1\n    };\n  },\n  computed: {\n    ...mapGetters([\"getProducts\", \"getProductsLoading\", \"getNumberPageProducts\"])\n  },\n  async created() {\n    // Get Products\n    this.$store.dispatch(\"SetProductsAPI\");\n\n    // Get Category\n    const CategoriesAxios = await GetCategories();\n    this.Categories = CategoriesAxios.data;\n  },\n  methods: {\n    // Search Icon\n    FocusInHandleChange() {\n      this.$refs.iconSearch.style.display = \"none\";\n    },\n    FocusOutHandleChange() {\n      this.$refs.iconSearch.style.display = \"block\";\n    },\n    // Switch Layout\n    SwitchLayoutHandleClick() {\n      this.isLayout = !this.isLayout;\n      console.log(this.isLayout);\n    },\n    // Category\n    CategoryHandleClick(id) {\n      this.categoryActive = id;\n      const Products = this.Products;\n      this.$store.dispatch(\"CategoryFood\", {\n        categoryId: id,\n        data: Products\n      });\n    }\n  }\n};","map":{"version":3,"names":["GetCategories","mapGetters","name","data","isLayout","Products","Categories","categoryActive","computed","created","$store","dispatch","CategoriesAxios","methods","FocusInHandleChange","$refs","iconSearch","style","display","FocusOutHandleChange","SwitchLayoutHandleClick","console","log","CategoryHandleClick","id","categoryId"],"sources":["D:\\Project Space\\FastFoodVuejs\\fast-food-client\\src\\pages\\ShopPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"Products-Container\">\r\n    <div class=\"container\">\r\n      <div class=\"row\">\r\n        <div class=\"col-lg-3 bg-danger p-3 text-light\">\r\n          <div class=\"Search w-100 p-2 bg-light mb-3\">\r\n            <input\r\n              type=\"text\"\r\n              name=\"\"\r\n              id=\"SearchInput\"\r\n              @focusin=\"FocusInHandleChange()\"\r\n              @focusout=\"FocusOutHandleChange()\"\r\n              placeholder=\"Tìm kiếm món ăn tại đây\"\r\n            />\r\n            <i\r\n              class=\"fa-solid fa-magnifying-glass text-danger\"\r\n              id=\"SearchIcon\"\r\n              ref=\"iconSearch\"\r\n            ></i>\r\n          </div>\r\n\r\n          <hr />\r\n\r\n          <div class=\"Category mt-2\">\r\n            <div class=\"Category-Title\">Danh Mục Món Ăn</div>\r\n            <ul>\r\n              <li\r\n                v-for=\"item in Categories\"\r\n                :key=\"item.id\"\r\n                @click=\"CategoryHandleClick(item.id)\"\r\n                :class=\"categoryActive == item.id && 'Active-Leftsite'\"\r\n              >\r\n                {{ item.name }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n\r\n          <hr />\r\n\r\n          <div class=\"Category mt-2\">\r\n            <div class=\"Category-Title\">Sắp Xếp Giá Cả</div>\r\n            <ul>\r\n              <!-- class=\"Active-Leftsite\" -->\r\n              <li>Dưới 50.000<sup>đ</sup></li>\r\n              <li>Từ 50.000<sup>đ</sup> đến 99.000<sup>đ</sup></li>\r\n              <li>Từ 100.000<sup>đ</sup> đến 500.000<sup>đ</sup></li>\r\n              <li>Trên 500.000<sup>đ</sup></li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"col-lg-9\">\r\n          <div class=\"Nav-Layout-Sort bg-light p-2\">\r\n            <div>\r\n              <span>\r\n                <i\r\n                  @click=\"!isLayout && SwitchLayoutHandleClick()\"\r\n                  :class=\"\r\n                    'fas fa-grip-horizontal ' + (isLayout && 'Active-Layout')\r\n                  \"\r\n                  id=\"IconGrids\"\r\n                ></i>\r\n              </span>\r\n              <span>\r\n                <i\r\n                  @click=\"isLayout && SwitchLayoutHandleClick()\"\r\n                  :class=\"'fa-solid fa-list ' + (!isLayout && 'Active-Layout')\"\r\n                  id=\"IconLists\"\r\n                ></i>\r\n              </span>\r\n            </div>\r\n\r\n            <div>\r\n              <select class=\"form-select\" aria-label=\"Default select example\">\r\n                <option selected>Mặc định</option>\r\n                <option value=\"1\">Giá thấp đến cao</option>\r\n                <option value=\"2\">Giá cao đến thấp</option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            :class=\"(!isLayout && 'mt-3') + ' row'\"\r\n            v-if=\"!getProductsLoading\"\r\n          >\r\n            <div\r\n              v-for=\"item in getProducts\"\r\n              :key=\"item.name\"\r\n              :class=\"isLayout ? 'col-lg-4 mt-3' : 'col-lg-12'\"\r\n            >\r\n              <div :class=\"'border-0 ' + (isLayout ? 'card' : 'row')\">\r\n                <img\r\n                  :class=\"isLayout ? 'card-img-top' : 'col-lg-4'\"\r\n                  :src=\"'/images/' + item.image\"\r\n                  alt=\"Card image\"\r\n                />\r\n                <div\r\n                  :class=\"\r\n                    isLayout\r\n                      ? 'card-body p-0 pt-3'\r\n                      : 'col-lg-8 d-flex justify-content-around flex-column'\r\n                  \"\r\n                >\r\n                  <h4 class=\"card-title Product-Name\">\r\n                    <b>{{ item.name }}</b>\r\n                  </h4>\r\n\r\n                  <span v-show=\"!isLayout\" class=\"card-text\">\r\n                    {{ item.desc }}\r\n                  </span>\r\n\r\n                  <h5 class=\"card-text text-danger\">\r\n                    <b>{{ item.price }}</b>\r\n                  </h5>\r\n                  <button class=\"btn btn-danger w-100\">Giỏ hàng</button>\r\n                </div>\r\n              </div>\r\n              <hr v-show=\"!isLayout\" />\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            v-else\r\n            class=\"d-flex h-100 justify-content-center align-items-center\"\r\n          >\r\n            <div class=\"spinner-border text-danger\" role=\"status\">\r\n              <span class=\"sr-only\">Loading...</span>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"Page-Number d-flex justify-content-center p-4\">\r\n            <button class=\"btn btn-danger\">\r\n              <i class=\"fa-solid fa-chevron-left\"></i>\r\n            </button>\r\n            <div class=\"Number-List\">\r\n              <button\r\n                class=\"btn btn-danger\"\r\n                v-for=\"number in getNumberPageProducts\"\r\n                :key=\"number\"\r\n              >\r\n                {{ number }}\r\n              </button>\r\n            </div>\r\n            <button class=\"btn btn-danger\">\r\n              <i class=\"fa-solid fa-chevron-right\"></i>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GetCategories } from \"../API/CategoriesRequest\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ShopPage\",\r\n  data() {\r\n    return {\r\n      isLayout: true,\r\n      Products: [],\r\n      Categories: [],\r\n      categoryActive: 1,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      \"getProducts\",\r\n      \"getProductsLoading\",\r\n      \"getNumberPageProducts\",\r\n    ]),\r\n  },\r\n  async created() {\r\n    // Get Products\r\n    this.$store.dispatch(\"SetProductsAPI\");\r\n\r\n    // Get Category\r\n    const CategoriesAxios = await GetCategories();\r\n    this.Categories = CategoriesAxios.data;\r\n  },\r\n\r\n  methods: {\r\n    // Search Icon\r\n    FocusInHandleChange() {\r\n      this.$refs.iconSearch.style.display = \"none\";\r\n    },\r\n    FocusOutHandleChange() {\r\n      this.$refs.iconSearch.style.display = \"block\";\r\n    },\r\n\r\n    // Switch Layout\r\n    SwitchLayoutHandleClick() {\r\n      this.isLayout = !this.isLayout;\r\n      console.log(this.isLayout);\r\n    },\r\n\r\n    // Category\r\n    CategoryHandleClick(id) {\r\n      this.categoryActive = id;\r\n      const Products = this.Products;\r\n      this.$store.dispatch(\"CategoryFood\", {\r\n        categoryId: id,\r\n        data: Products,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.Products-Container {\r\n  margin-top: 130px;\r\n  margin-bottom: 50px;\r\n}\r\n\r\n.Product-Name {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.Search {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  border-radius: 10px;\r\n}\r\n\r\n.Search > input {\r\n  background: none;\r\n  border: none;\r\n  width: 100%;\r\n}\r\n\r\n.Search > input:focus {\r\n  outline: none;\r\n}\r\n\r\n#SearchIcon {\r\n  font-size: 25px;\r\n}\r\n\r\n.Category > ul {\r\n  padding: 0;\r\n}\r\n\r\n.Category > ul > li {\r\n  list-style: none;\r\n  line-height: 40px;\r\n  /* background: blue; */\r\n  padding: 5px;\r\n  padding-left: 20px;\r\n  border-radius: 10px;\r\n  cursor: pointer;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.Category > ul > li:hover {\r\n  background: white;\r\n  color: rgb(220, 53, 69);\r\n}\r\n\r\n.Active-Leftsite {\r\n  background: white;\r\n  color: rgb(220, 53, 69);\r\n}\r\n\r\n.Category-Title {\r\n  font-weight: bold !important;\r\n  padding: 5px 0;\r\n  padding-left: 20px;\r\n}\r\n\r\n.Nav-Layout-Sort {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.Nav-Layout-Sort > span {\r\n  margin-left: 10px;\r\n}\r\n\r\n.Active-Layout {\r\n  background: rgb(220, 53, 69);\r\n  color: white;\r\n  padding: 5px;\r\n  border-radius: 5px;\r\n}\r\n\r\n#IconGrids {\r\n  font-size: 25px;\r\n  padding: 5px;\r\n  margin-right: 5px;\r\n  cursor: pointer;\r\n}\r\n#IconLists {\r\n  font-size: 25px;\r\n  padding: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.Number-List {\r\n  margin: 0 0px;\r\n  display: flex;\r\n}\r\n\r\n.Number-List > button {\r\n}\r\n</style>\r\n"],"mappings":"AA0JA,SAASA,aAAY,QAAS,0BAA0B;AACxD,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGP,UAAU,CAAC,CACZ,aAAa,EACb,oBAAoB,EACpB,uBAAuB,CACxB;EACH,CAAC;EACD,MAAMQ,OAAOA,CAAA,EAAG;IACd;IACA,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,gBAAgB,CAAC;;IAEtC;IACA,MAAMC,eAAc,GAAI,MAAMZ,aAAa,EAAE;IAC7C,IAAI,CAACM,UAAS,GAAIM,eAAe,CAACT,IAAI;EACxC,CAAC;EAEDU,OAAO,EAAE;IACP;IACAC,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAACC,KAAK,CAACC,UAAU,CAACC,KAAK,CAACC,OAAM,GAAI,MAAM;IAC9C,CAAC;IACDC,oBAAoBA,CAAA,EAAG;MACrB,IAAI,CAACJ,KAAK,CAACC,UAAU,CAACC,KAAK,CAACC,OAAM,GAAI,OAAO;IAC/C,CAAC;IAED;IACAE,uBAAuBA,CAAA,EAAG;MACxB,IAAI,CAAChB,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9BiB,OAAO,CAACC,GAAG,CAAC,IAAI,CAAClB,QAAQ,CAAC;IAC5B,CAAC;IAED;IACAmB,mBAAmBA,CAACC,EAAE,EAAE;MACtB,IAAI,CAACjB,cAAa,GAAIiB,EAAE;MACxB,MAAMnB,QAAO,GAAI,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACK,MAAM,CAACC,QAAQ,CAAC,cAAc,EAAE;QACnCc,UAAU,EAAED,EAAE;QACdrB,IAAI,EAAEE;MACR,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}