{"ast":null,"code":"import { GetCategories } from \"../API/CategoriesRequest\";\nimport { mapGetters } from \"vuex\";\nimport Products from \"../Database/db.js\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      Products: Products,\n      Categories: [],\n      newProducts: [],\n      trendingProducts: [],\n      // Dropdown Status\n      trendingDownStatus: false,\n      categoryDownStatus: false\n    };\n  },\n  async created() {\n    // Get Products\n    this.$store.dispatch(\"SetProductsAPI\");\n    this.Products = this.getAllProducts;\n\n    // Get Category\n    const CategoriesAxios = await GetCategories();\n    this.Categories = CategoriesAxios.data.filter(item => item.id != 1);\n  },\n  mounted() {\n    this.DropDownTrendingHandleClick();\n  },\n  computed: {\n    ...mapGetters([\"getAllProducts\"]),\n    getTrendingProduct() {\n      var productTrending = this.getAllProducts.filter(item => item.trending == true);\n      if (this.trendingDownStatus) {\n        return productTrending.slice(0, 4);\n      } else {\n        return productTrending;\n      }\n    },\n    getCategoriesProduct() {\n      var category = this.Categories;\n      if (!this.categoryDownStatus) {\n        return category.slice(0, 4);\n      } else {\n        return category;\n      }\n    },\n    getNewProducts() {\n      var NewProducts = this.Products.sort((a, b) => b.id - a.id);\n      NewProducts.slice(0, 8);\n      return NewProducts;\n    }\n  },\n  methods: {\n    // Dropdown\n    DropDownTrendingHandleClick() {\n      this.trendingDownStatus = !this.trendingDownStatus;\n    },\n    DropDownCategoryHandleClick() {\n      this.categoryDownStatus = !this.categoryDownStatus;\n    },\n    // Hover Category\n    CategoryHandleMouseOver(id) {\n      const TitleCategory = document.getElementById(`TitleCategory${id}`);\n      TitleCategory.classList.add(\"Category-Title\");\n    },\n    CategoryHandleMouseOut(id) {\n      const TitleCategory = document.getElementById(`TitleCategory${id}`);\n      TitleCategory.classList.remove(\"Category-Title\");\n    }\n  }\n};","map":{"version":3,"names":["GetCategories","mapGetters","Products","name","data","Categories","newProducts","trendingProducts","trendingDownStatus","categoryDownStatus","created","$store","dispatch","getAllProducts","CategoriesAxios","filter","item","id","mounted","DropDownTrendingHandleClick","computed","getTrendingProduct","productTrending","trending","slice","getCategoriesProduct","category","getNewProducts","NewProducts","sort","a","b","methods","DropDownCategoryHandleClick","CategoryHandleMouseOver","TitleCategory","document","getElementById","classList","add","CategoryHandleMouseOut","remove"],"sources":["D:\\Project Space\\FastFoodVuejs\\fast-food-client\\src\\pages\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"Home-Container\">\r\n    <div class=\"Banner\">\r\n      <img src=\"/images/combohappy149.webp\" alt=\"\" />\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN THỊNH HÀNH</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div\r\n          v-for=\"item in getTrendingProduct\"\r\n          :key=\"item.name\"\r\n          class=\"col-lg-3\"\r\n        >\r\n          <div v-if=\"item.trending\" class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div\r\n          v-if=\"\r\n            this.getAllProducts.filter((item) => item.trending == true).length >\r\n            4\r\n          \"\r\n          class=\"text-center p-3\"\r\n          @click=\"DropDownTrendingHandleClick()\"\r\n        >\r\n          <i\r\n            v-if=\"trendingDownStatus\"\r\n            style=\"cursor: pointer\"\r\n            class=\"fa-solid fa-chevron-down fa-2x\"\r\n          ></i>\r\n          <i\r\n            v-else\r\n            style=\"cursor: pointer\"\r\n            class=\"fa-solid fa-chevron-up fa-2x\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"bg-danger text-light pt-3 pb-3\">\r\n      <div class=\"container mt-5 mb-5\">\r\n        <div class=\"Title-List mb-3 text-end\">\r\n          <h3>DANH MỤC MÓN ĂN</h3>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div\r\n            v-for=\"item in getCategoriesProduct\"\r\n            :key=\"item.id\"\r\n            class=\"col-lg-3 position-relative\"\r\n            style=\"cursor: pointer\"\r\n          >\r\n            <router-link :to=\"{ name: 'Category', params: { id: item.id } }\">\r\n              <div\r\n                class=\"card border-0 mb-4 category\"\r\n                @mouseover=\"CategoryHandleMouseOver(item.id)\"\r\n                @mouseout=\"CategoryHandleMouseOut(item.id)\"\r\n              >\r\n                <img\r\n                  class=\"card-img-top\"\r\n                  :src=\"'/images/' + item.image\"\r\n                  alt=\"Card image\"\r\n                />\r\n                <div\r\n                  class=\"position-absolute text-danger\"\r\n                  style=\"display: none\"\r\n                  :id=\"`TitleCategory${item.id}`\"\r\n                >\r\n                  {{ item.name }}\r\n                </div>\r\n              </div>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        v-if=\"Categories.length > 4\"\r\n        class=\"text-center p-3\"\r\n        @click=\"DropDownCategoryHandleClick()\"\r\n      >\r\n        <i\r\n          v-if=\"!categoryDownStatus\"\r\n          style=\"cursor: pointer\"\r\n          class=\"fa-solid fa-chevron-down fa-2x\"\r\n        ></i>\r\n        <i\r\n          v-else\r\n          style=\"cursor: pointer\"\r\n          class=\"fa-solid fa-chevron-up fa-2x\"\r\n        ></i>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN MỚI NHẤT</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div v-for=\"item in getNewProducts\" :key=\"item.name\" class=\"col-lg-3\">\r\n          <div class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center p-3\">\r\n          <i class=\"fa-solid fa-chevron-down fa-2x\"></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GetCategories } from \"../API/CategoriesRequest\";\r\nimport { mapGetters } from \"vuex\";\r\nimport Products from \"../Database/db.js\";\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      Products: Products,\r\n      Categories: [],\r\n      newProducts: [],\r\n      trendingProducts: [],\r\n\r\n      // Dropdown Status\r\n      trendingDownStatus: false,\r\n      categoryDownStatus: false,\r\n    };\r\n  },\r\n  async created() {\r\n    // Get Products\r\n    this.$store.dispatch(\"SetProductsAPI\");\r\n    this.Products = this.getAllProducts;\r\n\r\n    // Get Category\r\n    const CategoriesAxios = await GetCategories();\r\n    this.Categories = CategoriesAxios.data.filter((item) => item.id != 1);\r\n  },\r\n  mounted() {\r\n    this.DropDownTrendingHandleClick();\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getAllProducts\"]),\r\n\r\n    getTrendingProduct() {\r\n      var productTrending = this.getAllProducts.filter(\r\n        (item) => item.trending == true\r\n      );\r\n      if (this.trendingDownStatus) {\r\n        return productTrending.slice(0, 4);\r\n      } else {\r\n        return productTrending;\r\n      }\r\n    },\r\n\r\n    getCategoriesProduct() {\r\n      var category = this.Categories;\r\n      if (!this.categoryDownStatus) {\r\n        return category.slice(0, 4);\r\n      } else {\r\n        return category;\r\n      }\r\n    },\r\n\r\n    getNewProducts() {\r\n      var NewProducts = this.Products.sort((a, b) => b.id - a.id);\r\n      NewProducts.slice(0, 8);\r\n      return NewProducts;\r\n    },\r\n  },\r\n  methods: {\r\n    // Dropdown\r\n    DropDownTrendingHandleClick() {\r\n      this.trendingDownStatus = !this.trendingDownStatus;\r\n    },\r\n    DropDownCategoryHandleClick() {\r\n      this.categoryDownStatus = !this.categoryDownStatus;\r\n    },\r\n\r\n    // Hover Category\r\n    CategoryHandleMouseOver(id) {\r\n      const TitleCategory = document.getElementById(`TitleCategory${id}`);\r\n      TitleCategory.classList.add(\"Category-Title\");\r\n    },\r\n    CategoryHandleMouseOut(id) {\r\n      const TitleCategory = document.getElementById(`TitleCategory${id}`);\r\n      TitleCategory.classList.remove(\"Category-Title\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.Home-Container {\r\n  margin-top: 88px;\r\n}\r\n\r\n.Banner > img {\r\n  width: 100%;\r\n}\r\n\r\n.Product-Name {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.Title-List h3 {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.category {\r\n  position: relative;\r\n}\r\n\r\n.category > img {\r\n  border-radius: 0px;\r\n}\r\n\r\n.Category-Title {\r\n  font-size: 20px;\r\n  display: block !important;\r\n  z-index: 100;\r\n  bottom: 0;\r\n  text-align: center;\r\n  width: 100%;\r\n  background: white;\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n</style>\r\n"],"mappings":"AA+IA,SAASA,aAAY,QAAS,0BAA0B;AACxD,SAASC,UAAS,QAAS,MAAM;AACjC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,QAAQ,EAAEA,QAAQ;MAClBG,UAAU,EAAE,EAAE;MACdC,WAAW,EAAE,EAAE;MACfC,gBAAgB,EAAE,EAAE;MAEpB;MACAC,kBAAkB,EAAE,KAAK;MACzBC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,gBAAgB,CAAC;IACtC,IAAI,CAACV,QAAO,GAAI,IAAI,CAACW,cAAc;;IAEnC;IACA,MAAMC,eAAc,GAAI,MAAMd,aAAa,EAAE;IAC7C,IAAI,CAACK,UAAS,GAAIS,eAAe,CAACV,IAAI,CAACW,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,EAAC,IAAK,CAAC,CAAC;EACvE,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,2BAA2B,EAAE;EACpC,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGnB,UAAU,CAAC,CAAC,gBAAgB,CAAC,CAAC;IAEjCoB,kBAAkBA,CAAA,EAAG;MACnB,IAAIC,eAAc,GAAI,IAAI,CAACT,cAAc,CAACE,MAAM,CAC7CC,IAAI,IAAKA,IAAI,CAACO,QAAO,IAAK,IAAG,CAC/B;MACD,IAAI,IAAI,CAACf,kBAAkB,EAAE;QAC3B,OAAOc,eAAe,CAACE,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;MACpC,OAAO;QACL,OAAOF,eAAe;MACxB;IACF,CAAC;IAEDG,oBAAoBA,CAAA,EAAG;MACrB,IAAIC,QAAO,GAAI,IAAI,CAACrB,UAAU;MAC9B,IAAI,CAAC,IAAI,CAACI,kBAAkB,EAAE;QAC5B,OAAOiB,QAAQ,CAACF,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;MAC7B,OAAO;QACL,OAAOE,QAAQ;MACjB;IACF,CAAC;IAEDC,cAAcA,CAAA,EAAG;MACf,IAAIC,WAAU,GAAI,IAAI,CAAC1B,QAAQ,CAAC2B,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACd,EAAC,GAAIa,CAAC,CAACb,EAAE,CAAC;MAC3DW,WAAW,CAACJ,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;MACvB,OAAOI,WAAW;IACpB;EACF,CAAC;EACDI,OAAO,EAAE;IACP;IACAb,2BAA2BA,CAAA,EAAG;MAC5B,IAAI,CAACX,kBAAiB,GAAI,CAAC,IAAI,CAACA,kBAAkB;IACpD,CAAC;IACDyB,2BAA2BA,CAAA,EAAG;MAC5B,IAAI,CAACxB,kBAAiB,GAAI,CAAC,IAAI,CAACA,kBAAkB;IACpD,CAAC;IAED;IACAyB,uBAAuBA,CAACjB,EAAE,EA