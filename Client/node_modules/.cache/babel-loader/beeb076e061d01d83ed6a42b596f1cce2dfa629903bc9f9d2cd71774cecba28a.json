{"ast":null,"code":"import { GetCategories } from \"../API/CategoriesRequest\";\nimport { mapGetters } from \"vuex\";\nimport Products from \"../Database/db.js\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      Products: Products,\n      Categories: []\n    };\n  },\n  async created() {\n    // Get Products\n    this.$store.dispatch(\"SetProductsAPI\");\n\n    // Get Category\n    const CategoriesAxios = await GetCategories();\n    this.Categories = CategoriesAxios.data.filter(item => item.id != 1);\n    this.newProducts = this.getAllProducts.sort((a, b) => b.id - a.id);\n  },\n  computed: {\n    ...mapGetters([\"getAllProducts\", \"getProductsLoading\", \"getNumberPageProducts\"]),\n    getNewProducts() {\n      return this.getAllProducts;\n    }\n  }\n};","map":{"version":3,"names":["GetCategories","mapGetters","Products","name","data","Categories","created","$store","dispatch","CategoriesAxios","filter","item","id","newProducts","getAllProducts","sort","a","b","computed","getNewProducts"],"sources":["D:\\Project Space\\FastFoodVuejs\\fast-food-client\\src\\pages\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"Home-Container\">\r\n    <div class=\"Banner\">\r\n      <img src=\"/images/combohappy149.webp\" alt=\"\" />\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN THỊNH HÀNH</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div v-for=\"item in getAllProducts\" :key=\"item.name\" class=\"col-lg-3\">\r\n          <div v-if=\"item.trending\" class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"bg-danger text-light pt-3 pb-3\">\r\n      <div class=\"container mt-5 mb-5\">\r\n        <div class=\"Title-List mb-3 text-end\">\r\n          <h3>DANH MỤC MÓN ĂN</h3>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div v-for=\"item in Categories\" :key=\"item.id\" class=\"col-lg-3\">\r\n            <div class=\"card border-0 mb-4 category\">\r\n              <img\r\n                class=\"card-img-top\"\r\n                :src=\"'/images/' + item.image\"\r\n                alt=\"Card image\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN MỚI NHẤT</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div v-for=\"item in getNewProducts\" :key=\"item.name\" class=\"col-lg-3\">\r\n          <div class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GetCategories } from \"../API/CategoriesRequest\";\r\nimport { mapGetters } from \"vuex\";\r\nimport Products from \"../Database/db.js\";\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      Products: Products,\r\n      Categories: [],\r\n    };\r\n  },\r\n  async created() {\r\n    // Get Products\r\n    this.$store.dispatch(\"SetProductsAPI\");\r\n\r\n    // Get Category\r\n    const CategoriesAxios = await GetCategories();\r\n    this.Categories = CategoriesAxios.data.filter((item) => item.id != 1);\r\n\r\n    this.newProducts = this.getAllProducts.sort((a, b) => b.id - a.id);\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      \"getAllProducts\",\r\n      \"getProductsLoading\",\r\n      \"getNumberPageProducts\",\r\n    ]),\r\n\r\n    getNewProducts() {\r\n      return this.getAllProducts;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.Home-Container {\r\n  margin-top: 88px;\r\n}\r\n\r\n.Banner > img {\r\n  width: 100%;\r\n}\r\n\r\n.Product-Name {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.Title-List h3 {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.category {\r\n  position: relative;\r\n}\r\n\r\n.category > img {\r\n  border-radius: 0px;\r\n}\r\n</style>\r\n"],"mappings":"AAmFA,SAASA,aAAY,QAAS,0BAA0B;AACxD,SAASC,UAAS,QAAS,MAAM;AACjC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,QAAQ,EAAEA,QAAQ;MAClBG,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,gBAAgB,CAAC;;IAEtC;IACA,MAAMC,eAAc,GAAI,MAAMT,aAAa,EAAE;IAC7C,IAAI,CAACK,UAAS,GAAII,eAAe,CAACL,IAAI,CAACM,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,EAAC,IAAK,CAAC,CAAC;IAErE,IAAI,CAACC,WAAU,GAAI,IAAI,CAACC,cAAc,CAACC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACL,EAAC,GAAII,CAAC,CAACJ,EAAE,CAAC;EACpE,CAAC;EACDM,QAAQ,EAAE;IACR,GAAGjB,UAAU,CAAC,CACZ,gBAAgB,EAChB,oBAAoB,EACpB,uBAAuB,CACxB,CAAC;IAEFkB,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACL,cAAc;IAC5B;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}