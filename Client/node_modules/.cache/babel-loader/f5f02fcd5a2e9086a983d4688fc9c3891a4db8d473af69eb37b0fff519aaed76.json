{"ast":null,"code":"import { GetCategories } from \"../API/CategoriesRequest\";\nimport { mapGetters } from \"vuex\";\nimport Products from \"../Database/db.js\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      Products: Products,\n      Categories: [],\n      newProducts: [],\n      trendingProducts: [],\n      // Dropdown Status\n      trendingDownStatus: false\n    };\n  },\n  async created() {\n    // Get Products\n    this.$store.dispatch(\"SetProductsAPI\");\n    this.Products = this.getAllProducts;\n\n    // Get Category\n    const CategoriesAxios = await GetCategories();\n    this.Categories = CategoriesAxios.data.filter(item => item.id != 1);\n    this.newProducts = this.Products.sort((a, b) => b.id - a.id).slice(0, 8);\n  },\n  mounted() {\n    this.DropDownTrendingHandleClick();\n  },\n  computed: {\n    ...mapGetters([\"getAllProducts\"]),\n    getTrendingProduct() {\n      var productTrending = this.getAllProducts.filter(item => item.trending == true);\n      if (this.trendingDownStatus) {\n        return productTrending.slice(0, 4);\n      } else {\n        return productTrending;\n      }\n    },\n    getNewProducts() {\n      return this.getAllProducts;\n    }\n  },\n  methods: {\n    // Dropdown\n    DropDownTrendingHandleClick() {\n      this.trendingDownStatus = !this.trendingDownStatus;\n    }\n  }\n};","map":{"version":3,"names":["GetCategories","mapGetters","Products","name","data","Categories","newProducts","trendingProducts","trendingDownStatus","created","$store","dispatch","getAllProducts","CategoriesAxios","filter","item","id","sort","a","b","slice","mounted","DropDownTrendingHandleClick","computed","getTrendingProduct","productTrending","trending","getNewProducts","methods"],"sources":["D:\\Project Space\\FastFoodVuejs\\fast-food-client\\src\\pages\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"Home-Container\">\r\n    <div class=\"Banner\">\r\n      <img src=\"/images/combohappy149.webp\" alt=\"\" />\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN THỊNH HÀNH</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div\r\n          v-for=\"item in getTrendingProduct\"\r\n          :key=\"item.name\"\r\n          class=\"col-lg-3\"\r\n        >\r\n          <div v-if=\"item.trending\" class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div\r\n          v-if=\"\r\n            this.getAllProducts.filter((item) => item.trending == true).length >\r\n            4\r\n          \"\r\n          class=\"text-center p-3\"\r\n          @click=\"DropDownTrendingHandleClick()\"\r\n        >\r\n          <i\r\n            v-if=\"trendingDownStatus\"\r\n            style=\"cursor: pointer\"\r\n            class=\"fa-solid fa-chevron-down fa-2x\"\r\n          ></i>\r\n          <i\r\n            v-else\r\n            style=\"cursor: pointer\"\r\n            class=\"fa-solid fa-chevron-up fa-2x\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"bg-danger text-light pt-3 pb-3\">\r\n      <div class=\"container mt-5 mb-5\">\r\n        <div class=\"Title-List mb-3 text-end\">\r\n          <h3>DANH MỤC MÓN ĂN</h3>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div\r\n            v-for=\"item in Categories\"\r\n            :key=\"item.id\"\r\n            class=\"col-lg-3 position-relative\"\r\n          >\r\n            <div class=\"card border-0 mb-4 category\">\r\n              <img\r\n                class=\"card-img-top\"\r\n                :src=\"'/images/' + item.image\"\r\n                alt=\"Card image\"\r\n              />\r\n              <div class=\"position-absolute Category-Title\">San phamr</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"text-center p-3\">\r\n        <i class=\"fa-solid fa-chevron-down fa-2x\"></i>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN MỚI NHẤT</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div v-for=\"item in newProducts\" :key=\"item.name\" class=\"col-lg-3\">\r\n          <div class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center p-3\">\r\n          <i class=\"fa-solid fa-chevron-down fa-2x\"></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GetCategories } from \"../API/CategoriesRequest\";\r\nimport { mapGetters } from \"vuex\";\r\nimport Products from \"../Database/db.js\";\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      Products: Products,\r\n      Categories: [],\r\n      newProducts: [],\r\n      trendingProducts: [],\r\n\r\n      // Dropdown Status\r\n      trendingDownStatus: false,\r\n    };\r\n  },\r\n  async created() {\r\n    // Get Products\r\n    this.$store.dispatch(\"SetProductsAPI\");\r\n    this.Products = this.getAllProducts;\r\n\r\n    // Get Category\r\n    const CategoriesAxios = await GetCategories();\r\n    this.Categories = CategoriesAxios.data.filter((item) => item.id != 1);\r\n\r\n    this.newProducts = this.Products.sort((a, b) => b.id - a.id).slice(0, 8);\r\n  },\r\n  mounted() {\r\n    this.DropDownTrendingHandleClick();\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getAllProducts\"]),\r\n\r\n    getTrendingProduct() {\r\n      var productTrending = this.getAllProducts.filter(\r\n        (item) => item.trending == true\r\n      );\r\n      if (this.trendingDownStatus) {\r\n        return productTrending.slice(0, 4);\r\n      } else {\r\n        return productTrending;\r\n      }\r\n    },\r\n\r\n    getNewProducts() {\r\n      return this.getAllProducts;\r\n    },\r\n  },\r\n  methods: {\r\n    // Dropdown\r\n    DropDownTrendingHandleClick() {\r\n      this.trendingDownStatus = !this.trendingDownStatus;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.Home-Container {\r\n  margin-top: 88px;\r\n}\r\n\r\n.Banner > img {\r\n  width: 100%;\r\n}\r\n\r\n.Product-Name {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.Title-List h3 {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.category {\r\n  position: relative;\r\n}\r\n\r\n.category > img {\r\n  border-radius: 0px;\r\n}\r\n</style>\r\n"],"mappings":"AAqHA,SAASA,aAAY,QAAS,0BAA0B;AACxD,SAASC,UAAS,QAAS,MAAM;AACjC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,QAAQ,EAAEA,QAAQ;MAClBG,UAAU,EAAE,EAAE;MACdC,WAAW,EAAE,EAAE;MACfC,gBAAgB,EAAE,EAAE;MAEpB;MACAC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,gBAAgB,CAAC;IACtC,IAAI,CAACT,QAAO,GAAI,IAAI,CAACU,cAAc;;IAEnC;IACA,MAAMC,eAAc,GAAI,MAAMb,aAAa,EAAE;IAC7C,IAAI,CAACK,UAAS,GAAIQ,eAAe