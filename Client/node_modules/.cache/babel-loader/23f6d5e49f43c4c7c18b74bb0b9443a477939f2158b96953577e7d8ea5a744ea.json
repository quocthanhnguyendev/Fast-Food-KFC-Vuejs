{"ast":null,"code":"import { GetCategories } from \"../API/CategoriesRequest\";\nimport { mapGetters } from \"vuex\";\nimport Products from \"../Database/db.js\";\nexport default {\n  name: \"HomePage\",\n  data() {\n    return {\n      Products: Products,\n      Categories: [],\n      newProducts: [],\n      // Dropdown Status\n      trendingDownStatus: false\n    };\n  },\n  async created() {\n    // Get Products\n    this.$store.dispatch(\"SetProductsAPI\");\n    this.Products = this.getAllProducts;\n\n    // Get Category\n    const CategoriesAxios = await GetCategories();\n    this.Categories = CategoriesAxios.data.filter(item => item.id != 1);\n    this.newProducts = this.Products.sort((a, b) => b.id - a.id).slice(0, 8);\n  },\n  computed: {\n    ...mapGetters([\"getAllProducts\", \"getProductsLoading\", \"getNumberPageProducts\"]),\n    getTrendingProduct() {\n      return this.getAllProducts.filter(item => item.trending == true);\n    },\n    getNewProducts() {\n      return this.getAllProducts;\n    }\n  },\n  methods: {\n    DropDownTrendingHandleClick() {\n      this.trendingDownStatus = !this.trendingDownStatus;\n      if (this.trendingDownStatus) {\n        console.log(1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["GetCategories","mapGetters","Products","name","data","Categories","newProducts","trendingDownStatus","created","$store","dispatch","getAllProducts","CategoriesAxios","filter","item","id","sort","a","b","slice","computed","getTrendingProduct","trending","getNewProducts","methods","DropDownTrendingHandleClick","console","log"],"sources":["D:\\Project Space\\FastFoodVuejs\\fast-food-client\\src\\pages\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"Home-Container\">\r\n    <div class=\"Banner\">\r\n      <img src=\"/images/combohappy149.webp\" alt=\"\" />\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN THỊNH HÀNH</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div\r\n          v-for=\"item in getTrendingProduct\"\r\n          :key=\"item.name\"\r\n          class=\"col-lg-3\"\r\n        >\r\n          <div v-if=\"item.trending\" class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center p-3\" @click=\"DropDownTrendingHandleClick()\">\r\n          <i class=\"fa-solid fa-chevron-down fa-2x\"></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"bg-danger text-light pt-3 pb-3\">\r\n      <div class=\"container mt-5 mb-5\">\r\n        <div class=\"Title-List mb-3 text-end\">\r\n          <h3>DANH MỤC MÓN ĂN</h3>\r\n        </div>\r\n\r\n        <div class=\"row\">\r\n          <div v-for=\"item in Categories\" :key=\"item.id\" class=\"col-lg-3\">\r\n            <div class=\"card border-0 mb-4 category\">\r\n              <img\r\n                class=\"card-img-top\"\r\n                :src=\"'/images/' + item.image\"\r\n                alt=\"Card image\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"text-center p-3\">\r\n        <i class=\"fa-solid fa-chevron-down fa-2x\"></i>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"container mt-5 mb-5\">\r\n      <div class=\"Title-List mb-3\">\r\n        <h3>MÓN ĂN MỚI NHẤT</h3>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div v-for=\"item in newProducts\" :key=\"item.name\" class=\"col-lg-3\">\r\n          <div class=\"card border-0\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"'/images/' + item.image\"\r\n              alt=\"Card image\"\r\n            />\r\n            <div class=\"card-body p-0 pt-3 pb-3\">\r\n              <h4 class=\"card-title Product-Name\">\r\n                <b>{{ item.name }}</b>\r\n              </h4>\r\n              <h5 class=\"card-text text-danger\">\r\n                <b>{{ item.price }}</b>\r\n              </h5>\r\n              <p class=\"card-text\">{{ item.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center p-3\">\r\n          <i class=\"fa-solid fa-chevron-down fa-2x\"></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { GetCategories } from \"../API/CategoriesRequest\";\r\nimport { mapGetters } from \"vuex\";\r\nimport Products from \"../Database/db.js\";\r\nexport default {\r\n  name: \"HomePage\",\r\n  data() {\r\n    return {\r\n      Products: Products,\r\n      Categories: [],\r\n      newProducts: [],\r\n\r\n      // Dropdown Status\r\n      trendingDownStatus: false,\r\n    };\r\n  },\r\n  async created() {\r\n    // Get Products\r\n    this.$store.dispatch(\"SetProductsAPI\");\r\n    this.Products = this.getAllProducts;\r\n\r\n    // Get Category\r\n    const CategoriesAxios = await GetCategories();\r\n    this.Categories = CategoriesAxios.data.filter((item) => item.id != 1);\r\n\r\n    this.newProducts = this.Products.sort((a, b) => b.id - a.id).slice(0, 8);\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      \"getAllProducts\",\r\n      \"getProductsLoading\",\r\n      \"getNumberPageProducts\",\r\n    ]),\r\n\r\n    getTrendingProduct() {\r\n      return this.getAllProducts.filter((item) => item.trending == true);\r\n    },\r\n\r\n    getNewProducts() {\r\n      return this.getAllProducts;\r\n    },\r\n  },\r\n  methods: {\r\n    DropDownTrendingHandleClick() {\r\n      this.trendingDownStatus = !this.trendingDownStatus;\r\n      if (this.trendingDownStatus) {\r\n        console.log(1);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.Home-Container {\r\n  margin-top: 88px;\r\n}\r\n\r\n.Banner > img {\r\n  width: 100%;\r\n}\r\n\r\n.Product-Name {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.Title-List h3 {\r\n  font-family: \"Oswald\", sans-serif;\r\n}\r\n\r\n.category {\r\n  position: relative;\r\n}\r\n\r\n.category > img {\r\n  border-radius: 0px;\r\n}\r\n</style>\r\n"],"mappings":"AAgGA,SAASA,aAAY,QAAS,0BAA0B;AACxD,SAASC,UAAS,QAAS,MAAM;AACjC,OAAOC,QAAO,MAAO,mBAAmB;AACxC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,QAAQ,EAAEA,QAAQ;MAClBG,UAAU,EAAE,EAAE;MACdC,WAAW,EAAE,EAAE;MAEf;MACAC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd;IACA,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,gBAAgB,CAAC;IACtC,IAAI,CAACR,QAAO,GAAI,IAAI,CAACS,cAAc;;IAEnC;IACA,MAAMC,eAAc,GAAI,MAAMZ,aAAa,EAAE;IAC7C,IAAI,CAACK,UAAS,GAAIO,eAAe,CAACR,IAAI,CAACS,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACC,EAAC,IAAK,CAAC,CAAC;IAErE,IAAI,CAACT,WAAU,GAAI,IAAI,CAACJ,QAAQ,CAACc,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACH,EAAC,GAAIE,CAAC,CAACF,EAAE,CAAC,CAACI,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC;EAC1E,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGnB,UAAU,CAAC,CACZ,gBAAgB,EAChB,oBAAoB,EACpB,uBAAuB,CACxB,CAAC;IAEFoB,kBAAkBA,CAAA,EAAG;MACnB,OAAO,IAAI,CAACV,cAAc,CAACE,MAAM,CAAEC,IAAI,IAAKA,IAAI,CAACQ,QAAO,IAAK,IAAI,CAAC;IACpE,CAAC;IAEDC,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACZ,cAAc;IAC5B;EACF,CAAC;EACDa,OAAO,EAAE;IACPC,2BAA2BA,CAAA,EAAG;MAC5B,IAAI,CAAClB,kBAAiB,GAAI,CAAC,IAAI,CAACA,kBAAkB;MAClD,IAAI,IAAI,CAACA,kBAAkB,EAAE;QAC3BmB,OAAO,CAACC,GAAG,CAAC,CAAC,CAAC;MAChB;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}